	<span class="k">return</span> <span class="n">org</span><span class="p">.</span><span class="n">next</span><span class="p">;</span>
<span class="p">}</span>

<span class="kt">int</span> <span class="nf">is_empty</span><span class="p">()</span> <span class="p">{</span>
	<span class="k">return</span> <span class="n">get_head</span><span class="p">()</span><span class="o">==</span><span class="nb">NULL</span><span class="p">;</span>
