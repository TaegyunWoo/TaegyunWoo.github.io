</span><span class="k">typedef</span> <span class="kt">int</span> <span class="n">Element</span><span class="p">;</span>
<span class="n">Element</span> <span class="n">list</span><span class="p">[</span><span class="n">MAX_LIST_SIZE</span><span class="p">];</span>
