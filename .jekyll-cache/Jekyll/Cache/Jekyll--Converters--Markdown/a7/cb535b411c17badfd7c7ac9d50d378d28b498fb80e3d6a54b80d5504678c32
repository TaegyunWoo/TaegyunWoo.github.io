  <span class="kd">static</span> <span class="kt">int</span> <span class="n">sizeOfEdge</span><span class="o">;</span> <span class="c1">//ê°„ì„ ê°œìˆ˜</span>
  <span class="kd">static</span> <span class="kt">int</span> <span class="n">startNode</span><span class="o">;</span> <span class="c1">//ì‹œì‘ë…¸ë“œ(index)</span>
  <span class="kd">static</span> <span class="nc">ArrayList</span><span class="o">&lt;</span><span class="nc">ArrayList</span><span class="o">&lt;</span><span class="nc">Node</span><span class="o">&gt;&gt;</span> <span class="n">graph</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">ArrayList</span><span class="o">&lt;&gt;();</span> <span class="c1">//ê·¸ë˜í”„</span>
<p><span class="cm">/**</p>
